{
  "name": "fileinasnap",
  "version": "1.0.0",
  "description": "AI-powered modular file organization and memory management platform",
  "main": "orchestrator/bmad-cli.ts",
  "type": "module",
  "scripts": {
    "start": "node orchestrator/bmad-cli.js pipeline",
    "dev": "tsx --watch orchestrator/bmad-cli.ts pipeline --plan=pro",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "bmad": "node orchestrator/bmad-cli.js",
    "supervisor": "echo 'Supervisor functionality available in development mode'",
    
    "container:flows": "node orchestrator/bmad-cli.js pipeline pro",
    "container:file": "node orchestrator/bmad-cli.js trigger file.uploaded standard",
    "container:journal": "node orchestrator/bmad-cli.js trigger journal.created pro", 
    "container:admin": "node orchestrator/bmad-cli.js trigger admin.override_panel enterprise",
    
    "list:containers": "node orchestrator/bmad-cli.js list containers pro",
    "list:features": "node orchestrator/bmad-cli.js list features pro"
  },
  "keywords": [
    "ai",
    "file-organization",  
    "memory-management",
    "agents",
    "typescript",
    "automation"
  ],
  "author": "FileInASnap Team",
  "license": "MIT",
  "dependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.11.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}