{
  "name": "fileinasnap",
  "version": "1.0.0",
  "description": "AI-powered modular file organization and memory management platform",
  "main": "mobile_classification_app/bmadOrchestrator.ts",
  "type": "module",
  "scripts": {
    "start": "tsx mobile_classification_app/bmadOrchestrator.ts build-all",
    "dev": "tsx --watch mobile_classification_app/bmadOrchestrator.ts build-all",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "agent:file": "tsx mobile_classification_app/bmadOrchestrator.ts build-file-intelligence",
    "agent:journal": "tsx mobile_classification_app/bmadOrchestrator.ts build-journaling",
    "agent:relationship": "tsx mobile_classification_app/bmadOrchestrator.ts build-relationship",
    "agent:story": "tsx mobile_classification_app/bmadOrchestrator.ts build-stories",
    "agent:analytics": "tsx mobile_classification_app/bmadOrchestrator.ts analytics",
    "container:flows": "tsx mobile_classification_app/containerFlowOrchestrator.ts demo-all",
    "container:file": "tsx mobile_classification_app/containerFlowOrchestrator.ts file-upload",
    "container:journal": "tsx mobile_classification_app/containerFlowOrchestrator.ts journal-entry", 
    "container:admin": "tsx mobile_classification_app/containerFlowOrchestrator.ts admin-override"
  },
  "keywords": [
    "ai",
    "file-organization",  
    "memory-management",
    "agents",
    "typescript",
    "automation"
  ],
  "author": "FileInASnap Team",
  "license": "MIT",
  "dependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.11.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}