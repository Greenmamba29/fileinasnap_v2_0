{
  "name": "fileinasnap",
  "version": "1.0.0",
  "description": "AI-powered modular file organization and memory management platform",
  "main": "mobile_classification_app/bmadOrchestrator.ts",
  "type": "module",
  "scripts": {
    "start": "tsx orchestrator/bmad-cli.ts pipeline",
    "dev": "tsx --watch orchestrator/bmad-cli.ts pipeline",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "bmad": "tsx orchestrator/bmad-cli.ts",
    "supervisor": "node -e \"const {AgentSupervisor} = require('./orchestrator/agent-supervisor.ts'); const supervisor = new AgentSupervisor(); supervisor.monitorSystemHealth().then(metrics => console.log('System Health:', metrics));\"",
    
    "container:flows": "tsx orchestrator/bmad-cli.ts pipeline",
    "container:file": "tsx orchestrator/bmad-cli.ts trigger file.uploaded",
    "container:journal": "tsx orchestrator/bmad-cli.ts trigger journal.created", 
    "container:admin": "tsx orchestrator/bmad-cli.ts trigger admin.override_panel",
    "container:story": "tsx orchestrator/bmad-cli.ts trigger story.generation_requested",
    "container:relationship": "tsx orchestrator/bmad-cli.ts trigger image.face_detected",
    
    "list:containers": "tsx orchestrator/bmad-cli.ts list containers",
    "list:features": "tsx orchestrator/bmad-cli.ts list features"
  },
  "keywords": [
    "ai",
    "file-organization",  
    "memory-management",
    "agents",
    "typescript",
    "automation"
  ],
  "author": "FileInASnap Team",
  "license": "MIT",
  "dependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.11.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}